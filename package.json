{
  "name": "trigo-react-app",
  "version": "1.3.1",
  "description": "Heavily opinionated react app boilerplate",
  "main": "dist/trigo-react-app.js",
  "license": "MIT",
  "repository": "git@github.com:trigo-at/trigo-react-app.git",
  "author": "trigo GmbH <office@trigo.at> (http://www.trigo.at)",
  "bugs": {
    "url": "https://github.com/trigo-at/trigo-react-app/issues"
  },
  "homepage": "https://github.com/trigo-at/trigo-react-app#readme",
  "scripts": {
    "clean": "rm -rf dist/*",
    "lint": "eslint '**/*.js'",
    "test": "cross-env NODE_ENV=test mocha -R spec --require babel-register --require src/specs.setup.js $(find ./src -name '*.specs.js*')",
    "cover": "cross-env NODE_NEV=test node --harmony-async-await ./node_modules/istanbul/lib/cli.js --include-all-sources cover -- ./server/run.test.js",
    "compile": "cross-env NODE_ENV=production webpack --progress --profile --colors",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "npm run clean & npm run compile"
  },
  "keywords": [
    "react",
    "router5",
    "react-router5",
    "redux",
    "trigo",
    "boilerplate"
  ],
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router5": "^4.0.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-router5": "^4.2.3",
    "redux-thunk": "^2.2.0",
    "router5": "^4.5.1",
    "styled-components": "^1.4.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "asset-require-hook": "^1.0.2",
    "autoprefixer": "^6.7.5",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "cross-env": "^3.1.4",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-react-trigo": "^1.0.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.10.1",
    "istanbul": "^0.4.5",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "semantic-release": "^6.3.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
  }
}
